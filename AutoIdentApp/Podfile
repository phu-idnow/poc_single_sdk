  pod 'OpenCV2', '~> 3.4.1'
  pod 'Sentry', :git => 'https://github.com/getsentry/sentry-cocoa.git', :tag => '4.1.0'
  pod 'lottie-ios'

  # Pods for AutoIdentApp

end
