  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'Alamofire', '~> 4.7', :inhibit_warnings => true
  pod 'AlamofireLogger'
  pod 'Starscream', '~> 3.0.2'
